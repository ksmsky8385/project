    "SCR_EST_2023"
  ],

  "TARGET_COLUMN": "",

  "TEST_SIZE": 0.2,

  "MISSING_VALUE_STRATEGY": {
    "enable": false,
    "zero_threshold_ratio": 0.5,
    "remove_zero_by_column": false,
    "remove_zero_by_row": true,
    "method": "drop",
    "fill_value": 0
