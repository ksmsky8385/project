        "min_samples": 5
      },
      "AgglomerativeClustering": {
        "n_clusters": 2,
        "linkage": "ward"
      }
    }
  },

  "WINDOW_CONFIG": {
    "enabled": true,
    "window_size": 3
  },

  "PARAMS": {
    "full": {
      "n_estimators": 400,
      "max_depth": 4,
      "learning_rate": 0.05,
      "subsample": 0.7,
