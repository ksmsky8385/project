{
  "n_clusters": {
    "range": [2, 6],
    "type": "int"
  },
  "n_estimators": {
    "range": [100, 1000],
    "type": "int"
  },
  "max_depth": {
    "range": [3, 30],
    "type": "int"
  },
  "min_samples_split": {
    "range": [2, 20],
    "type": "int"
  },
  "min_samples_leaf": {
    "range": [1, 10],
    "type": "int"
  },
  "max_features": {
    "choices": [null, "sqrt", "log2"],
    "type": "categorical"
  },
  "random_state": {
    "fixed": 42,
    "type": "fixed"
  },
  "n_jobs": {
    "fixed": -1,
    "type": "fixed"
  }
